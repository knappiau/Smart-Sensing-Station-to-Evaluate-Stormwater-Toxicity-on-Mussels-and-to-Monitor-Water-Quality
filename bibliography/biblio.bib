  journal = {Water Environment Research},
  author = {Chapman, Cameron and Horner, Richard R},
  month = {feb},
  year = {2010},
  keywords = {best management practices, bioretention, low-impact development, runoff, stormwater, water quality monitoring},
  pages = {109--119},
}


