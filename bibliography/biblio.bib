@article{ahern_respiratory_1999,
  title = {{Respiratory, acid–base and metabolic responses of the freshwater crayfish {Cherax} destructor to lead contamination}},
  volume = {124},
  issn = {1095-6433},
  url = {http://www.sciencedirect.com/science/article/pii/S1095643399001014},
  doi = {10.1016/S1095-6433(99)00101-4},
  abstract = {The effects of lead contamination on respiratory, acid–base and metabolic status were investigated in the freshwater Australian crayfish, Cherax destructor. Lead has been found to perturb sodium balance in C. destructor and it seemed likely, given the link between ion regulation and gas exchange in aquatic crustaceans, that respiration and acid–base balance would be compromised. Exposure of C. destructor to either 100 or 0.5 mg l−1 Pb in the water caused a reduction in oxygen consumption (ṀO2) of at least 50\% within 24 h, and after 21 days ṀO2 was only 30\% of pre-exposure levels. The reduced ṀO2 was accompanied by a decrease in heart rate from 66±9 to 29±6 beats min−1 in the initial 24 h but ventilation rate remained unchanged (23±5 beats min−1). Concomitantly there was a marked reduction of the oxygen transfer factor (TO2) across the gills from 2.69×10−2 μmol Pa−1, typical of crustaceans, to 1.08×10−2 μmol Pa−1 in impacted crayfish after 21 days exposure. Despite the reduced ṀO2 and TO2, there was almost no change in the acid–base status of Pb impacted crayfish in comparison to controls. While part of the normal response of C. destructor to hypoxia is a hyperventilation leading to lowered ṀO2, the apparent hypoxia caused by Pb impact did not elicit any increase in ventilation and thereby no attempt was made to maintain a ‘normoxic’ ṀO2. Metabolism was not supplemented by anaerobiosis and thus overall energetic demand must have been lowered. This response strongly suggests that Pb promotes a hypometabolic state as well as an elevation of the threshold PO2 at which anaerobiosis would be initiated. Lead contamination would therefore exclude C. destructor from waters prone to hypoxia.},
  number = {2},
  urldate = {2017-08-31},
  journal = {Comparative Biochemistry and Physiology Part A: Molecular \& Integrative Physiology},
  author = {Ahern, Mark D. and Morris, Steve},
  month = {oct},
  year = {1999},
  keywords = {Acid–base, Cherax, Crayfish, Hypometabolism, hypoxia, Lead, Metal, Oxygen, respiration, Ventilation},
  pages = {105--111},
  file = {ScienceDirect Full Text PDF:/Users/knappi/Library/Application Support/Zotero/Profiles/5kwbj6it.default/zotero/storage/9NDFUNTD/Ahern and Morris - 1999 - Respiratory, acid–base and metabolic responses of .pdf:application/pdf;ScienceDirect Snapshot:/Users/knappi/Library/Application Support/Zotero/Profiles/5kwbj6it.default/zotero/storage/ECXM76XJ/S1095643399001014.html:text/html},
}


@article{ames_habitat_2015,
  title = {{Habitat mediates the outcome of a cleaning symbiosis for a facultatively burrowing crayfish}},
  volume = {60},
  issn = {1365-2427},
  url = {http://onlinelibrary.wiley.com/doi/10.1111/fwb.12559/abstract},
  doi = {10.1111/fwb.12559},
  abstract = {* Branchiobdellidans are small, symbiotic annelids (Clitellata) that can have positive effects on the survival and growth of the crayfish host. We hypothesised that branchiobdellidans would be particularly beneficial to crayfish that move to underground burrows (characterised by poor water quality) as surface waters recede. * In surface water mesocosms, the symbioses were mutualistic, with worm (Cambarincola barbarae.) density increasing over time and crayfish (Procambarus clarkii) growing faster when worms were present. * Contrary to previous studies, the outcome did not shift from mutualism to parasitism at high worm densities. * In burrowing chambers in the laboratory, the symbiotic relationship was lost, as worm density decreased by 92\% and crayfish growth ceased. Similarly, in burrows excavated in field experiments, worm density decreased by 84\%. * Rather than the symbiotic relationship increasing survival when crayfish moved underground, this crayfish species may be faced with a trade-off between the benefits of a mutualism with worms while on the surface and protection from heat stress and desiccation by burrowing when water levels are low.},
  language = {en},
  number = {5},
  urldate = {2017-08-31},
  journal = {Freshw Biol},
  author = {Ames, Catlin W. and Helms, Brian S. and Stoeckel, James A.},
  month = {may},
  year = {2015},
  keywords = {Branchiobdellida, burrow, commensalism, mutualism, Procambarus clarkii},
  pages = {989--999},
  file = {Full Text PDF:/Users/knappi/Library/Application Support/Zotero/Profiles/5kwbj6it.default/zotero/storage/7FAQ4FJA/Ames et al. - 2015 - Habitat mediates the outcome of a cleaning symbios.pdf:application/pdf;Snapshot:/Users/knappi/Library/Application Support/Zotero/Profiles/5kwbj6it.default/zotero/storage/DBVUPRZU/abstract.html:text/html},
}


@article{borcherding_dreissena-monitor_1994,
  title = {{The “{Dreissena}-{Monitor}” – {First} {Results} on the {Application} of this {Biological} {Early} {Warning} {System} in the {Continuous} {Monitoring} of {Water} {Quality}}},
  volume = {29},
  copyright = {© IWA Publishing 1994},
  issn = {0273-1223, 1996-9732},
  url = {http://wst.iwaponline.com/content/29/3/199},
  abstract = {Skip to Next Section The “Dreissena-Monitor” is a biological early warning system used in the continuous monitoring of water quality. The system is based on a computer assessment of valve movements in two groups of up to 42 zebra mussels (Dreissena polymorpha). Every 5 minutes, the percentage of open mussels and the average number of valve movements per mussel are calculated to describe the activity behaviour of the mussels. Rapid changes in both parameters normally reflect the onset of stress. More than 5½ years of experience in the continuous monitoring of water quality have revealed (1) that the “Dreissena-Monitor” is suited to large rivers like the Rhine as well as small rivers like the Erft or Ruhr, (2) that a lack of technical interruptions guarantees reliable, unattended operation, (3) that normally less than 3 hours of weekly maintenance are required, (4) that the validity check of the primary measurements was capable of identifying dead mussels and poorly aligned measurement components, (5) that the system can operate under unfavourable conditions (e.g. temperatures below 5° C), (6) that mortality rates were low and correlated positively with temperature, and (7) that the alarm thresholds provided a high degree of statistical security.},
  language = {en},
  number = {3},
  urldate = {2017-08-31},
  journal = {Water Science and Technology},
  author = {Borcherding, Jost and Volpers, Markus},
  month = {feb},
  year = {1994},
  pages = {199--201},
  file = {Snapshot:/Users/knappi/Library/Application Support/Zotero/Profiles/5kwbj6it.default/zotero/storage/4PBKGVPF/199.html:text/html},
}


